{
  "name": "habith-be",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "watch-static": "nodemon --watch src/public --exec npm run copy-static-assets",
    "dev": "cross-env NODE_ENV=development concurrently \"ts-node-dev --respawn --transpile-only ./src/server.ts\" \"npm:watch-static\"",
    "build-ts": "tsc",
    "build": "cross-env NODE_ENV=production npm run build-ts && npm run copy-static-assets",
    "build:dev": "cross-env NODE_ENV=development npm run build-ts && npm run copy-static-assets",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules\\typeorm\\cli.js -f ./src/ormconfig.ts",
    "migration:gen": "node ./node_modules/.bin/typeorm migration:generate --config dist/ormconfig.js",
    "migration:run": "node ./node_modules/.bin/typeorm migration:run --config dist/ormconfig.js",
    "migration:gen:win": "npm run typeorm migration:generate -- -n migrate",
    "migration:run:win": "npm run typeorm migration:run",
    "migration:revert:win": "npm run typeorm migration:revert",
    "test": "cross-env NODE_ENV=testing jest --silent=false --coverage --verbose --forceExit --detectOpenHandles",
    "docker:build": "docker build --file ./Dockerfile -t iot-project-mid-led .",
    "test:build": "cross-env NODE_ENV=posttesting jest --config=build.jest.config.js --verbose",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "swagger-autogen": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register swagger.ts"
  },
  "keywords": [
    "express",
    "nodejs"
  ],
  "author": "Matthew Christopher Albert",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-connection-string": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.18.8",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.38",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^7.2.13",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^16.3.0",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.11",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "^6.2.0",
    "eslint": "^8.9.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.0.5",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.4"
  }
}
